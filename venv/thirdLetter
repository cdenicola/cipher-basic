import string

def thirdltr(str):
    str = str.translate({ord(c): None for c in string.whitespace})
    print(str)
    newStr = ""
    for i in range(1, len(str)):
        if i % 3 == 0:
            newStr += str[i-1]
    print()
    print(newStr)
    return

thirdltr("""WIGSAEPTNENECVRZNALTL   QIDXBOKJNTJORUVSFALON   PYIRTS
GFCAIOYUNNDSHLIWEDNKEQURCBEMHD   ROTFTHSIE   APFETAOPTHLHVJEXER
CJOAIF   XCMGKOEMDFZEPGRBAN   GHAFIMWEERURDKINYCCCALHN
ZOIQUNAVTORETLLUNLEWISKGIBEAGNSPCITE
TAGBRAYMTVWHQIEPFRGWIPVNBZG.""")